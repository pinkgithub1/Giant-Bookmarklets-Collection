javascript:%7Blet%20numThumbs%20=%200;%0A%0Aconst%20dl%20=%20link%20=%3E%20%7B%0A%20%20let%20vid%20=%20window.location.pathname.split('/').pop();%0A%0A%20%20fetch(new%20Request(link)).then(response%20=%3E%20response.blob()).then(b%20=%3E%20%7B%0A%20%20%20%20const%20objectURL%20=%20URL.createObjectURL(b);%0A%20%20%20%20const%20tag%20=%20document.createElement('a');%0A%20%20%20%20tag.href%20=%20objectURL;%0A%20%20%20%20tag.download%20=%20%60$%7Bvid%7D.mp4%60;%0A%20%20%20%20tag.click();%0A%20%20%7D)%0A%7D%0A%0Aconst%20dlNext%20=%20()%20=%3E%20%7B%0A%20%20dl(document.querySelector('video').src);%0A%20%20const%20left%20=%20document.querySelector(%60%5Bdata-e2e='arrow-left'%5D:not(%5Bdisabled%5D)%60);%0A%20%20if%20(left)%20%7B%0A%20%20%20%20left.click();%0A%20%20%20%20setTimeout(dlNext,%201500);%0A%20%20%7D%0A%7D%0A%0Aconst%20scrollDown%20=%20()%20=%3E%20%7B%0A%20%20document.querySelector(%60%5Bdata-e2e='user-post-item-list'%5D%60).scrollIntoView(false);%0A%20%20const%20thumbs%20=%20document.querySelectorAll(%60%5Bdata-e2e='user-post-item'%5D%60);%0A%20%20if%20(thumbs.length%20%3E%20numThumbs)%20%7B%0A%20%20%20%20numThumbs%20=%20thumbs.length;%0A%20%20%20%20setTimeout(scrollDown,%201000);%0A%20%20%7D%20else%20%7B%0A%20%20%20%20console.log(numThumbs%20+%20'%20videos%20found');%0A%20%20%20%20%5B...thumbs%5D.pop().querySelector('a').click();%0A%20%20%20%20setTimeout(dlNext,%201500);%0A%20%20%7D%0A%7D%0A%0AscrollDown();%7D
